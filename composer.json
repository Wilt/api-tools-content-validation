{
    "name": "laminas-api-tools/api-tools-content-validation",
    "description": "Laminas Module providing incoming content validation",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "validation"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-content-validation/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-content-validation",
        "rss": "https://github.com/laminas-api-tools/api-tools-content-validation/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev",
            "dev-develop": "1.3-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0",
        "laminas/laminas-eventmanager": "~2.3",
        "laminas/laminas-filter": "~2.3",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-inputfilter": "~2.4",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-servicemanager": "~2.3",
        "laminas/laminas-stdlib": "~2.3",
        "laminas/laminas-validator": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-loader": "~2.3",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\ContentValidation\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\ContentValidation\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-content-validation": "self.version"
    }
}
